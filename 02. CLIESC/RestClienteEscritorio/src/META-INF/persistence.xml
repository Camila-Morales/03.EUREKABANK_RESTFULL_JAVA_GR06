<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="RestClienteEscritorioPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>ec.edu.monster.modelo.Sucursal</class>
    <class>ec.edu.monster.modelo.Moneda</class>
    <class>ec.edu.monster.modelo.Tipomovimiento</class>
    <class>ec.edu.monster.modelo.Cuenta</class>
    <class>ec.edu.monster.modelo.Movimiento</class>
    <class>ec.edu.monster.modelo.Cliente</class>
    <class>ec.edu.monster.modelo.Empleado</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/wssoap?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
